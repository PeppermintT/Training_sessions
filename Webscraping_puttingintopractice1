    "#Now that we have the correct set of <div></div> tags, we need to extract the policy data from \n",
    "#with the <span></span> tags.\n",
    "category_list = [] # define a blank list for storing the category data\n",
    "blog_name = \"Cal Newport\" # define a variable for storing the charity's name\n",
    "\n",
    "for tag in policy_section.find_all(\"a\"): # for each set of span tags in the policy section\n",
    "    policy = tag.text # extract the text from the tag\n",
    "    observation = [blog_name, policy] # combine charity name and a policy\n",
    "    category_list.append(observation) # append the charity name and policy to the blank list\n",
    "    \n",
    "category_list # view list of policies for the charity (long format)"
